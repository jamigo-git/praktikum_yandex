{
    "env": {
		"browser": true,
		"es6": true,
		"es2017": true
    },
    "extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "tsconfig.json",
		"tsconfigRootDir": "."
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
        "no-unused-vars": "off"
    }
}
